# MongoDB 连接配置
# 本地开发可以使用默认值：mongodb://localhost:27017/tgbot
MONGODB_URI=mongodb://localhost:27017/tgbot

# JWT 密钥
# 生产环境请使用强密钥
JWT_SECRET=your_jwt_secret_key

# 应用配置
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NODE_ENV=development

# 其他配置
# 是否启用 API 日志
ENABLE_API_LOGS=true
# API 速率限制（每分钟请求数）
MAX_REQUESTS_PER_MINUTE=100 