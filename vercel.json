{
  "version": 2,
  "buildCommand": "NODE_ENV=development pnpm install --no-frozen-lockfile && NODE_ENV=production pnpm build",
  "installCommand": "NODE_ENV=development pnpm install --no-frozen-lockfile",
  "framework": "nextjs",
  "outputDirectory": "apps/web/.next",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "v1": false
    }
  },
  "ignoreCommand": "git diff HEAD^ HEAD --quiet",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
} 