{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "buildCommand": "npm run build",
                "outputDirectory": "Web/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/"
        },
        {
            "src": "/(.+)",
            "dest": "/$1"
        }
    ]
}