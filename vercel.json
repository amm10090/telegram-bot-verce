{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/test.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/start.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/webhook",
            "destination": "/api/webhook.js"
        },
        {
            "source": "/api/stats",
            "destination": "/api/stats.js"
        },
        {
            "source": "/api/test",
            "destination": "/api/test.js"
        },
        {
            "source": "/api/start",
            "destination": "/api/start.js"
        },
        {
            "source": "/static/(.*)",
            "destination": "/dist/static/$1"
        },
        {
            "source": "/((?!api/).*)",
            "destination": "/dist/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        },
        {
            "source": "/static/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}