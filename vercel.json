{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/test.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/start.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist",
                "buildCommand": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "^/assets/(.*)",
            "dest": "/assets/$1",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "^/api/webhook",
            "dest": "/api/webhook.js",
            "methods": [
                "POST",
                "OPTIONS"
            ]
        },
        {
            "src": "^/api/test",
            "dest": "/api/test.js",
            "methods": [
                "GET",
                "OPTIONS"
            ]
        },
        {
            "src": "^/api/start",
            "dest": "/api/start.js",
            "methods": [
                "GET",
                "OPTIONS"
            ]
        },
        {
            "src": "^/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "^/(.*)",
            "dest": "/index.html",
            "headers": {
                "cache-control": "public, max-age=0, must-revalidate"
            }
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin-when-cross-origin"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "camera=(), microphone=(), geolocation=(), interest-cohort=()"
                }
            ]
        }
    ]
}