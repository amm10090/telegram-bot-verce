{
    "version": 2,
    "routes": [
        {
            "src": "/api/bot",
            "dest": "/api/bot.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        }
    ],
    "functions": {
        "api/bot.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}