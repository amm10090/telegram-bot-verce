{
    "version": 2,
    "buildCommand": "turbo run build",
    "outputDirectory": "apps/web/dist",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/server/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/apps/web/$1"
        }
    ]
}